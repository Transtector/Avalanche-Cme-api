ie_svg(image, viewbox = '0 0 1024 1024', color = #fff)
	preamble = "data:image/svg+xml,"
	svg_open = "%3Csvg%20xmlns='http://www.w3.org/2000/svg'%20viewBox='" + viewbox + "'%3E"
	group_open = "%3Cg%20fill='" + color + "' stroke='" + color + "'%3E"
	group_close = "%3C/g%3E"
	svg_close = "%3C/svg%3E"
	url(preamble + svg_open + group_open + ie-svg-images[image] + group_close + svg_close)


fg-color = #4D4D4D
warning-color = #FFD42A
error-color = #FF2222

// inline SVG images
header 
	.tab
		bottom -1px
	.branding
		background-image ie_svg('logo', '0 0 128 128', 'white')
		
.notification .title
	background-image ie_svg('alarm', '0 0 80 80', warning-color)
	
.ch .ch-history-badge::before
	background-image ie_svg('plot-trace', '-5 -5 50 40') 

.th-badge
	background-image ie_svg('alarm', '0 0 80 80', warning-color)
.th-badge.alarm
	background-image ie_svg('alarm', '0 0 80 80', error-color)



// icon images
.icon-dashboard
	background-image ie_svg('dashboard', '0 0 1000 1000', fg-color)
.icon-alarm
	background-image ie_svg('alarm', '0 0 80 80', fg-color)
.icon-settings
	background-image ie_svg('settings2', '0 0 1024 1024', fg-color)
.icon-logout
	background-image ie_svg('logout2', '0 0 1024 1024', fg-color)
.icon-user
	background-image ie_svg('user', '0 0 278 278', '#6f6f6f')
.icon-key
	background-image ie_svg('key', '0 0 509 509', '#6f6f6f')
	
.panel#dashboard
	icon-small-viewport = '-512 -512 2048 2048'
	.icon-download
		background-image ie_svg('download2', icon-small-viewport)
	.icon-chevron-left
		background-image ie_svg('chevron-left', icon-small-viewport)
	.icon-chevron-right
		background-image ie_svg('chevron-right', icon-small-viewport)
	.icon-chevron-down
		background-image ie_svg('chevron-down', '-128 -128 1152 1152') // just slightly smaller
	.icon-cross
		background-image ie_svg('cross', icon-small-viewport)

	.icon-play
		background-image ie_svg('play')
	.icon-pause
		background-image ie_svg('pause')
	.icon-view-alarms
		background-image ie_svg('view-alarms', '0 0 80 80')
	.icon-record
		background-image ie_svg('record', '0 0 1024 1024', error-color)
	.icon-record-check
		background-image ie_svg('record-check', '0 0 1024 1024', error-color)

.panel#settings
	.icon-download
		background-image svg('download2', '0 0 1024 1024', '#6f6f6f')
	.icon-trash
		background-image svg('trash2', '0 0 1024 1024', '#6f6f6f')
	.icon-logbook
		background-image svg('logbook', '0 0 280 280')


// stand-alone page overrides
body.errorpage .enormous-font span
	background-image ie_svg('logo', '0 0 128 128', 'white')
