

.export
	position relative
	padding 10px
	table 
		width 100%
		border-collapse collapse
		background-color #fff
	th, td
		border 1px solid #ccc
		padding 0.25rem
		text-align left
		& span:last-child
			margin-left 1em
			font-style italic
	thead tr:last-child
		background-color rgba(button-bg-color, 0.25)
		th, td
			border-top-width 2px
	tbody tr:nth-child(even)
		background-color rgba(button-bg-color, 0.1)
	
.export > button
	position absolute
	right 10px
	top 10px
	width 48px
	border-radius 100%
	font-weight bold
	
.export .instructions
	position absolute
	top 40px
	left 50%
	margin-left -190px
	background #fff
	width 380px
	padding 0 10px 10px 10px
	border-radius 3px
	box-shadow 0 2px 9px 0 rgba(0,0,0,0.2)
	p
		margin-bottom 10px
	.buttons
		text-align right
	button
		width auto
		font-size 90%
		margin-left 20px
		padding 2px 10px 3px		

.loaderWrapper
	position absolute
	left 0
	top 0
	right 0
	bottom 0
	background-color rgba(button-bg-color, 0.25)


.loader
	margin 100px auto
	font-size 25px
	width 1em
	height 1em
	border-radius 50%
	position relative
	text-indent -9999em
	-webkit-animation load5 1.1s infinite ease
	animation load5 1.1s infinite ease
	-webkit-transform translateZ(0)
	-ms-transform translateZ(0)
	transform translateZ(0)

